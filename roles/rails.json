{
  "name": "rails",
  "description": "Configures Rails stack",
  "json_class": "Chef::Role",
  "chef_type": "role",
  "default_attributes": {
    "rbenv":{
      "rubies" : "2.1.1",
      "global" : "2.1.1",
      "gems" : {
        "2.1.1": [{"name": "bundler"}]
      }
    }
  },
  "run_list": [
    "recipe[ruby_build]",
    "recipe[rbenv::system]",
    "recipe[rails-dependencies]"
  ]
}